{
  "name": "bitbucket-changelog",
  "version": "1.0.0",
  "description": "A changelog generator for bitbucket.",
  "source": "index.js",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "dev": "node index.js",
    "lint": "standard",
    "build": "parcel build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Alejandroid17/bitbucket-changelog"
  },
  "keywords": [
    "changelog",
    "pull-request",
    "bitbucket",
    "log"
  ],
  "author": "Alejandroid17",
  "license": "MIT",
  "dependencies": {
    "dotenv": "16.0.3",
    "fs-extra": "11.1.0",
    "nunjucks": "3.2.3",
    "ora": "6.1.2"
  },
  "devDependencies": {
    "parcel": "2.8.3",
    "standard": "17.0.0"
  },
  "eslintConfig": {
    "extends": "./node_modules/standard/eslintrc.json"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "targets": {
    "main": {
      "optimize": true
    }
  }
}
